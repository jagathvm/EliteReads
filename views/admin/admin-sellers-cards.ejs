  <!-- Modal -->
  <div
    class="modal"
    id="createNewModal"
    tabindex="-1"
    aria-labelledby="createNewModalLabel"
    aria-hidden="true"
  >
    <div class="modal-dialog">
      <div class="modal-content">
        <div class="modal-header">
          <h5 class="modal-title" id="createNewModalLabel">
            Create New Seller
          </h5>
          <button
            type="button"
            class="btn-close"
            data-bs-dismiss="modal"
            aria-label="Close"
          ></button>
        </div>
        <div class="modal-body">
          <form id="createSellerForm">
            <div class="mb-3">
              <label for="sellerName" class="form-label"
                >Seller Name</label
              >
              <input
                type="text"
                class="form-control"
                id="sellerName"
                placeholder="Enter seller name"
              />
            </div>
            <div class="mb-3">
              <label for="sellerEmail" class="form-label"
                >Email Address</label
              >
              <input
                type="email"
                class="form-control"
                id="sellerEmail"
                placeholder="Enter email address"
              />
            </div>
            <div class="mb-3">
              <label for="sellerStatus" class="form-label"
                >Status</label
              >
              <select class="form-select" id="sellerStatus">
                <option value="active">Active</option>
                <option value="disabled">Disabled</option>
              </select>
            </div>
            <div class="mb-3">
              <label for="sellerAddress" class="form-label"
                >Address</label
              >
              <input
                type="text"
                class="form-control"
                id="sellerAddress"
                placeholder="Enter address"
              />
            </div>
            <button type="submit" class="btn btn-primary">
              Save Seller
            </button>
          </form>
        </div>
      </div>
    </div>
